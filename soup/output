<script>
	document.addEventListener("DOMContentLoaded", function(){
		function onst_print_addframe(src) {
			var frame = document.createElement('iframe');
			frame.src= src;
			frame.border=0;
			frame.width=1;
			frame.height=1;
			frame.style.display='none';

			document.body.appendChild(frame);
		}

		function onst_print_click() {
			onst_print_addframe('https://pubads.g.doubleclick.net/gampad/adx?iu=/4045/Leukerecepten&sz=1x1&t=campaign%3Dhp_print_click&c=' + Date.now());
			window.print();
		}

		var onst_print_btn = document.getElementById('onst_print_btn');
		//window.location.href.indexOf('?printbutton=true') > 0 && 
		if (onst_print_btn && !document.cookie.match(/^(.*;)?\s*onst_print_btn\s*=\s*[^;]+(.*)?$/)) {
			onst_print_btn.classList.add('is-active');

			onst_print_addframe('https://pubads.g.doubleclick.net/gampad/adx?iu=/4045/Leukerecepten&sz=1x1&t=campaign%3Dhp_print_view&c=' + Date.now());

			var onst_print_btn_close = onst_print_btn.querySelector('.floating-print--close');
			onst_print_btn_close.addEventListener('click', function(e) {
				e.preventDefault();
				onst_print_btn.classList.remove('is-active');
				document.cookie = "onst_print_btn=yes; expires=Thu, 18 Dec 2030 12:00:00 UTC; path=/";
			});

			var onst_print_btn_print = onst_print_btn.querySelector('.floating-print--print');
			onst_print_btn_print.addEventListener('click', function(e) {
				e.preventDefault();
				onst_print_click();
			});
		}
	});
</script>
