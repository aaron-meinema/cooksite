	    jQuery('.user-star-rating.user-can-vote li').click(function(e) {
	    	e.preventDefault();
	    	var li = jQuery(this);
	        var stars = jQuery(this).find('a').data('star-value');
	        var rating_stars = jQuery(this).parent('ul');
